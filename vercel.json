{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"

    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "crons": [
    {
      "path": "/api/payment-plan/check-installment",
      "schedule":"0 6 * * *"
    }, {
      "path": "/api/payment-plan/check-installment",
      "schedule":"0 18 * * *"
    }
  ]
}